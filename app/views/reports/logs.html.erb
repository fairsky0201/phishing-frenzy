<% @page_title = "Log Details" %>

<h2>Apache Logs: <%= @campaign.name %></h2>

<%= button_to("Download Logs", {:controller => 'reports', :action => 'download_logs', :id => @campaign.id}) %>

<% counter = 1 %>
<% @apache_data[:logs].each do |log| %>
	<table>
		<tr class="<%= cycle('odd', 'even') %>">
			<td width="800">
				<p><%= simple_format(log) %></p>
				<% counter += 1 %>
			</td>
		</tr>
	</table>
<% end %>